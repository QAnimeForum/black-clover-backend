{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      "@nestjs/swagger"
  ],
  "assets": [{ "include": "languages/**/*", "watchAssets": true, "outDir": "dist/src/" }],
    "deleteOutDir": true,
    "watchAssets": true
  }
}
